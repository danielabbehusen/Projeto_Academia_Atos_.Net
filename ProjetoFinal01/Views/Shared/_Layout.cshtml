<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProgFit </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProjetoFinal01.styles.css" asp-append-version="true" />
    <link rel="icon" type="imagem/png" href="data:image/jpeg;base64," />

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-info border-bottom box-shadow">
            <div class="container-fluid">
                 <img class="navbar-brand" src="https://cdn-icons-png.flaticon.com/512/755/755298.png" style="width: 50px; height:50px;" />
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">ProgFit</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                   
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Alunos" asp-action="Index">Alunos</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Professor" asp-action="Index">Professores</a>
                        </li>

                         <li class="nav-item" style="margin-left: 910px;">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Login">Login</a>
                        </li>

                        

                        
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div >
        <main role="main">
            @RenderBody()
        </main>
    </div>

   <footer class=" mt-0 border-top footer text-muted bg-dark ">
        <div class="container" style="text-align:center;">
            Academia .NET ATOS/UFN &copy; 2022 | ProgFit | Daniel Abbehusen Miguel Cruz | <a href="https://www.linkedin.com/in/daniel-abbehusen-9721b8190/"> <img style="widht: 20px; height: 20px;" src ="https://cdn-icons.flaticon.com/png/512/3536/premium/3536505.png?token=exp=1642092789~hmac=63a7da9fe0523f4c51ffacefd3b7e634"/></a>  <a class="link-dark" href="https://github.com/danielabbehusen"> <img style="widht: 20px; height: 20px;" src="https://cdn-icons-png.flaticon.com/512/25/25231.png"/> </a>
            
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>



